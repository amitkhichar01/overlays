<% layout("./layouts/boilerplate.ejs") -%> <% pageTitle = "Overlays Clothing Official Store" %> <% user = user%> <% templateStyles = "/css/home.css" %> <% templateJavaScript = "/js/home.js" %>
<main class="home-main-container" data-scroll-container>
